
@model List<HCL.Academy.Model.Banners>

@*<div class="flexslider container">*@
    <ul class="slides">
        @*<li><img src="~/Images/Banner_ING_01.png" class="img-responsive" alt="" /></li>
        <li><img src="~/Images/Banner_ING_02.png" class="img-responsive" alt="" /></li>*@

        @if (Model != null && Model.Count > 0)
        {
            foreach (var item in Model)
            {
                <li>
                    <img src="data:image;base64,@item.bannerImageSRC" class="img-responsive" alt="" />
                    <img src="data:image;base64,@item.bannerImageSRC"  alt="Media Object" />
                </li>
            }
        }

    </ul>
@*</div>*@