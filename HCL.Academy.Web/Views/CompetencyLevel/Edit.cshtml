@model HCL.Academy.Model.Competence

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
<div id="body" class="form-horizontal">
    <section class="content-wrapper main-content clear-fix container">
        <table class="table table-bordered admin-onbrdtble">
            <tr>
                <td>
                    @Html.Label("Competency Level Name")
                </td>
                <td>
                    @Html.EditorFor(model => model.CompetenceName, new { htmlAttributes = new { @class = "form-control" } })
                </td>
                <td><input type="submit" value="Save" class="btn btn-default" /></td>
            </tr>
        </table>
        @Html.ValidationMessageFor(model => model.CompetenceName, "", new { @class = "text-danger" })
    </section>
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
</div>
}
