@model IEnumerable<HCL.Academy.Model.ErrorLogEntity>

<div class="table-responsive tabular-data">
    <div class="modal-body">
        <div class="container-fluid">
            <div class="detls-panel">
                
                <div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000" style="padding-top:110px">
                    <article>
                        <header>
                            <h1 style="font-size: 15px;font-weight:bolder;margin-bottom:10px">Exception List</h1>

                        </header>
                    </article>
                    <table class="table table-bordered table-striped" id="user-count">
                        <thead style="background-color:#326295;color:#fff">
                            <tr>
                                <td>Application Name</td>
                                <td>Partition Key</td>                           
                                <td>Message</td>
                                <td>Detail Message</td>
                                @*<td>Source</td>*@
                                <td>User Name</td>
                                <td>Time Stamp</td>
                                <td>Action</td>
                                
                            </tr>
                        </thead>
                        <tbody>
                            @if(Model.Count() > 0)
                            {
                                foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @item.ApplicationName
                                        </td>
                                        <td>
                                            @item.PartitionKey
                                        </td>
                                        <td>
                                            @item.Message
                                        </td>
                                        <td>
                                            @item.DetailMessage
                                        </td>
                                        @*<td>
                                            @item.Source
                                        </td>*@
                                        <td>
                                            @item.UserName
                                        </td>
                                        <td>
                                            @item.Timestamp
                                        </td>
                                        <td>@Html.ActionLink("Delete", "Delete", new { id = item.RowKey })</td>
                                    </tr>
                                }
                            }
                            else
                            { 
                                <tr>
                                    <td colspan="7" style="text-align: center;">No data to display</td>
                                </tr>
                            }

                        </tbody>
                    </table>
                    <br />
                    @*@if (Model.Count() == 0)
                    {

                        <a href="@Url.Action("Create", "RedisCache")" class="elements"><span>Add Configuration</span></a>
                    }*@
                </div>
            </div>
        </div>
    </div>
</div>

